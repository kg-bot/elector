<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Dmas" table="Dmas">
    <id name="dmaid" type="smallint" column="DmaId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="countryid" type="smallint" column="CountryId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dma" type="string" column="DMA" length="3" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="market" type="string" column="Market" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
