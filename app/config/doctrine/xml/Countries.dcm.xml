<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Countries" table="Countries">
    <id name="countryid" type="smallint" column="CountryId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="country" type="string" column="Country" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fips104" type="string" column="FIPS104" length="2" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="iso2" type="string" column="ISO2" length="2" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="iso3" type="string" column="ISO3" length="3" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ison" type="string" column="ISON" length="4" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="internet" type="string" column="Internet" length="2" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="capital" type="string" column="Capital" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mapreference" type="string" column="MapReference" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nationalitysingular" type="string" column="NationalitySingular" length="35" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nationalityplural" type="string" column="NationalityPlural" length="35" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="currency" type="string" column="Currency" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="currencycode" type="string" column="CurrencyCode" length="3" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="population" type="bigint" column="Population" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="title" type="string" column="Title" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comment" type="string" column="Comment" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
